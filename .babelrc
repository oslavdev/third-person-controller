{
    "presets": [
      "@babel/preset-env",
      ["@babel/preset-react", {
        "runtime": "automatic"
      }],
      "@babel/preset-typescript"
    ],
    "plugins": [
        "react-hot-loader/babel",
        [
            "module-resolver",
            {
                "alias": {
                    "@": "./src"
                }
            }
        ]
    ]
  }